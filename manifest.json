{
    "manifest_version": 3,
    "name": "Video To-Do List",
    "version": "1.0",
    "permissions": [
        "storage",
        "activeTab",
        "identity"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "icon16.png",
            "48": "icon48.png",
            "128": "icon128.png"
        }
    }
}